{
    "name": "napopravku/queue-monitor-lib",
    "description": "Queue Monitoring for Laravel Database Job Queue",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "v.s.novikov",
            "email": "v.novikov@napopravku.ru"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "illuminate/database": "^5.5|^6.0|^7.0|^8.0|^9.0",
        "illuminate/queue": "^5.5|^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0|^9.0",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "laravel/framework": "^5.5|^6.0|^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.3.2",
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0",
        "phpstan/phpstan": "^0.12.99|^1.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "napopravku/php-cs-fixer-config": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "napopravku\\QueueMonitor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "napopravku\\QueueMonitor\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "napopravku\\QueueMonitor\\Providers\\QueueMonitorProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
